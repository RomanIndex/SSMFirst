  <body class="nav-md">

    <div class="container body">
        <div class="main_container">
            <div class="right_col" role="main">
                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            <div class="x_content">

                                <table id="table" class="table table-hover table-bordered" cellspacing="0" width="100%"></table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="filter_div">
        <form class="form-inline">
            <a class="btn btn-default handle" id="export"><span>导出</span></a>

            <div class="form-group">
                <label class="control-label" for="name">keyWord：</label>
                <input type="text" class="form-control" id="name" name="name" placeholder="请输入要搜索关键词">
            </div>

            <a class="btn btn-default" id="query"><span>查询</span></a>
            <a class="btn btn-default" id="add"><span>新增</span></a>

            <button type="button" class="btn btn-sm" id="batch-edit">批量编辑</button>
            <button type="button" class="btn btn-success btn-sm" id="batch-save">批量保存</button>
        </form>
    </div>

</body>
